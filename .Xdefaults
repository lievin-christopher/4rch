!! Appearance
URxvt.termName: rxvt-unicode
URxvt.scrollBar: false
URxvt.depth: 32
URxvt.background: rgba:0000/0000/0000/cccc
URxvt.foreground: #B5A780
URxvt.cursorColor: #B5A780
URxvt.geometry: 108x25
URxvt.transparent: true
URxvt.shading: 20
!! Font prefferenes
URxvt.font: xft:Hack Nerd Font:pixelsize=13
URxvt.boldFont: xft:Hack Nerd Font:pixelsize=14:weight=bold
!! Larger history limit
URxvt.saveLines: 1000000
!! Scroll
URxvt.scrollTtyOutput:      false
URxvt.scrollWithBuffer:     true
URxvt.scrollTtyKeypress:    true
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
!! Perl extensions
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,matcher,resize-font
URxvt.url-select.launcher:  firefox-developer-edition
URxvt.url-select.underline: true
URxvt.iso14755 :            false
URxvt.iso14755_52:          false
URxvt.keysym.C-S-0x43:      eval:selection_to_clipboard
URxvt.keysym.C-S-0x56:      eval:paste_clipboard
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

! black
URxvt.color0:    #707070
URxvt.color8:    #C0C0C0

! red
URxvt.color1:    #E06C75
URxvt.color9:    #BF616A

! green
URxvt.color2:    #60B48A
URxvt.color10:   #98C379

! yellow
URxvt.color3:    #EBBE65
URxvt.color11:   #EBBE65

! blue
URxvt.color4:    #61AFEF
URxvt.color12:   #02C9FF

! magenta
URxvt.color5:    #C678DD
URxvt.color13:   #9285FF

! cyan
URxvt.color6:    #76E2FF
URxvt.color14:   #C0F2FF

! white
URxvt.color7:    #D8D8D8
URxvt.color15:   #D8D8D8
