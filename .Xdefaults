!! Appearance
URxvt.termName: rxvt-unicode
URxvt.scrollBar: false
URxvt.depth: 32
URxvt.background: rgba:0064/0061/0046/cccc
URxvt.foreground: #b5a780
URxvt.cursorColor: #b5a780
URxvt.geometry: 108x25
URxvt.transparent: true
URxvt.shading: 50
!! Font prefferenes
URxvt.font: xft:Hack Nerd Font:pixelsize=13
URxvt.boldFont: xft:Hack Nerd Font:pixelsize=14:weight=bold
!! Larger history limit
URxvt.saveLines: 1000000
!! Scroll
URxvt.scrollTtyOutput:      false
URxvt.scrollWithBuffer:     true
URxvt.scrollTtyKeypress:    true
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
!! Perl extensions
URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,matcher,resize-font
URxvt.url-select.launcher:  firefox-developer-edition
URxvt.url-select.underline: true
URxvt.iso14755 :            false
URxvt.iso14755_52:          false
URxvt.keysym.C-S-0x43:      eval:selection_to_clipboard
URxvt.keysym.C-S-0x56:      eval:paste_clipboard
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

! black
URxvt.color0:    rgba:0064/0061/0046/3333
URxvt.color8:    #709080

! red
URxvt.color1:    #bf616a
URxvt.color9:    #bf616a

! green
URxvt.color2:    #60b48a
URxvt.color10:   #92be6d

! yellow
URxvt.color3:    #ebbe65
URxvt.color11:   #ebbe65

! blue
URxvt.color4:    #02c9ff
URxvt.color12:   #02b9eb

! magenta
URxvt.color5:    #9285FF
URxvt.color13:   #9285ff

! cyan
URxvt.color6:    #76e2ff
URxvt.color14:   #c0f2ff

! white
URxvt.color7:    #dcdccc
URxvt.color15:   #9285FF
